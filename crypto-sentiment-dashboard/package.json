{
  "name": "crypto-sentiment-vite-cloudflare",
  "version": "1.0.0",
  "description": "A crypto sentiment dashboard using Vite, React, and Cloudflare Workers",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "npm run build && npx wrangler dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "wrangler deploy"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^4.4.0",
    "wrangler": "^4.19.1"
  }
}
